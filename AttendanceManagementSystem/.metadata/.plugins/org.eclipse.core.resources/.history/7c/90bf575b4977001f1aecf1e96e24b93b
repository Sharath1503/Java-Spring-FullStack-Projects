package com.ex.DAO;

import org.springframework.jdbc.core.JdbcTemplate;

import com.ex.StudentEntity;

public class attendedstudentsDAO {
	
private JdbcTemplate jdbcTemplate1;
	
	public void setJdbcTemplate1(JdbcTemplate jdbcTemplate) {
		this.jdbcTemplate1 = jdbcTemplate;
	}

	
	public int Attendancesave(StudentEntity student) {
		// TODO Auto-generated method stub
		return jdbcTemplate1.update("insert into attendedstudents(id,branch,studentsattended) values(?,?,?)", student.getSname(), student.getRoll(), student.getAge());
		
	}

}
